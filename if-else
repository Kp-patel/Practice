//checking number if it is positive or negative
#include<stdio.h>
void main()
{
    int no;
    
    printf("\n Enter no");
    scanf("%d",&no);

    if(no>0)
    printf("\n The number is positive.");
    else if(no==0)
    printf("\n The number is zero.");
    else
    {
        printf("\nThe number is negative.");
    }
}
